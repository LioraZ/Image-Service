@model ImageServiceWeb.Models.PhotosModel.PhotosModel
@{
    ViewBag.Title = "Photos";
}

<h2>Photos</h2>
<div class="row">
    <div class="col-xs-18 col-sm-6 col-md-3">
       @foreach (var item in Model.Photos)
       {
            <div class="thumbnail">
                <img src=@Url.Content(item.RelativeThumbnailPath) class="img-circle" alt=@item.Name>
                <div class="caption">
                    <h3>@item.Name</h3>
                    <p>@item.Date</p>
                    <p>
                        <a href="#" class="btn btn-primary" role="button">Button</a>
                        <a href="#" class="btn btn-danger" role="button">Button</a>
                    </p>
                </div>
            </div>
        }
    </div>
</div>


